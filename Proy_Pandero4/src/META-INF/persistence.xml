<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="Proy_Pandero">
		<class>Bean.SocioBean</class>
		<class>Bean.RolBean</class>
		<class>Bean.EstCivilBean</class>
		<class>Bean.AccesosBean</class>
		<class>Bean.MenuBean</class>
		<class>Bean.SexoBean</class>
		<class>Bean.UbigeoBean</class>
		<class>Bean.DepartamentoBean</class>
		<class>Bean.ProvinciaBean</class>
		<class>Bean.DistritoBean</class>
		<class>Bean.TipoObligacion</class>
 		<class>Bean.ObligacionBean</class>
 
		<properties>
   			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
          	<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/panderoDB"/>
   			<property name="eclipselink.jdbc.user" value="root"/>
   			<property name="eclipselink.jdbc.password" value="mysql"/>     
   			<property name="eclipselink.logging.level" value="FINE"/> 
		</properties>		
	</persistence-unit>
</persistence>
